{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "dev": "npm run start:web:watch & npm run start:server:watch",
    "build:server:watch": "tsc -w -p tsconfig.server.json ",
    "start:web:watch": "ng build --output-path dist/web --watch",
    "start:server:watch": "npm run build:server:watch & nodemon ./dist/server.js",
    "deploy:stage": "npm run deploy:web:stage & npm run deploy:server:stage",
    "deploy:web:stage": "ng build --prod --outputPath=\"../stage/web\" --deleteOutputPath=true",
    "deploy:server:stage": "tsc -p tsconfig.server.json --outDir ../stage --sourceMap false",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.0",
    "@angular/common": "~7.1.0",
    "@angular/compiler": "~7.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~7.1.0",
    "@angular/http": "~7.1.0",
    "@angular/platform-browser": "~7.1.0",
    "@angular/platform-browser-dynamic": "~7.1.0",
    "@angular/router": "~7.1.0",
    "@ng-select/ng-select": "^2.16.2",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.19.2",
    "rxjs": "~6.3.3",
    "shelljs": "^0.8.5",
    "tslib": "^1.9.0",
    "ws": "^6.2.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.6",
    "@angular/cli": "^15.1.4",
    "@angular/compiler-cli": "^10.0.12",
    "@angular/language-service": "~7.1.0",
    "@types/express": "^4.16.1",
    "@types/jasmine": "^2.8.16",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^29.3.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
